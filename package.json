{
  "name": "timedisparity",
  "displayName": "TimeDisparity",
  "description": "The temporal distance that divides by regions.",
  "version": "0.0.7",
  "publisher": "yeejiac",
  "engines": {
    "vscode": "^1.92.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yeejiac/TimeDisparity"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "timedisparity",
          "name": "Time Disparity"
        },
        {
          "id": "uriView",
          "name": "Generated URIs"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "myActivityBar",
          "title": "My Activity Bar",
          "icon": "resources/my-icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.showTime",
        "title": "Show Time"
      },
      {
        "command": "timedisparity.addBirthday",
        "title": "Add Birthday"
      },
      {
        "command": "timedisparity.addTimeZone",
        "title": "Add TimeZone"
      },
      {
          "command": "timedisparity.removeTimezone",
          "title": "Remove Timezone"
      },
      {
        "command": "extension.generateUri",
        "title": "Generate VSCode URI"
      }
    ],
    "menus": {
        "view/title": [
            {
                "command": "timedisparity.addBirthday",
                "title": "Add Birthday"
            },
            {
              "command": "timedisparity.addTimeZone",
              "title": "Add TimeZone"
            },
            {
              "command": "timedisparity.removeTimezone",
              "title": "Remove Timezone"
            }
        ],
        "editor/context": [
        {
          "command": "extension.generateUri",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "npm run compile && node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "moment-timezone": "^0.5.45"
  },
  "license": "MIT",
  "icon": "icon.png"
}
